<?xml version="1.0"?>
<configuration debug="false">
	<appender
			name="console"
			class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<Pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</Pattern>
		</encoder>
	</appender>
	<root level="error">
		<appender-ref ref="console"/>
	</root>
	<logger name="activerecord" level="debug"/>
</configuration>
